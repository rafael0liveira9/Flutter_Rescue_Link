<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Tema aplicado ao Android Window quando o modo escuro está ativado -->
    <style name="LaunchTheme" parent="@android:style/Theme.Black.NoTitleBar">
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>

    <!-- Tema aplicado ao Android Window após o processo ser iniciado no modo escuro -->
    <style name="NormalThemeNight" parent="@android:style/Theme.Black.NoTitleBar">
        <item name="android:windowBackground">?android:colorBackground</item>
    </style>
</resources>
